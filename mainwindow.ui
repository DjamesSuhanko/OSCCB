   <property name="styleSheet">
    <string notr="true">/* tipografia e cards */

QFrame[channelCard=&quot;true&quot;] {
  background: #2a2d34;
  border: 1px solid #3a3f47;
  border-radius: 12px;
}

/* títulos dos canais */
QLabel[channelTitle=&quot;true&quot;] {
  color: #cfd6dd;
  font-weight: 600;
  letter-spacing: 0.5px;
}

/* o “meter” vertical (progressbar) */
QProgressBar {
  border: 1px solid #3a3f47;
  border-radius: 12px;
  background: #1f2125;
}
QProgressBar::chunk {
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #001f4d, stop:1 #00ffff);;
}

/* botão MUDO (toggle) */
QPushButton[toggle=&quot;mute&quot;] {
  min-height: 36px;
  border-radius: 18px;
  border: 1px solid #3a3f47;
  color: #0b0d10;
  background: #29cc6a;      /* verde quando !checked */
  font-weight: 600;
}
QPushButton[toggle=&quot;mute&quot;]:!checked { background: #29cc6a; color: #0b0d10; }
QPushButton[toggle=&quot;mute&quot;]:checked  { background: #ff3b30; color: white;  }
QPushButton[toggle=&quot;mute&quot;]:pressed  { filter: brightness(0.9); }

/* botões de cena (em cima) */
QPushButton[scene=&quot;true&quot;] {
  min-height: 34px;
  padding: 0 14px;
  border-radius: 10px;
  border: 1px solid #3a3f47;
  background: #2f333a;
  color: #e9eef3;
}
QPushButton[scene=&quot;true&quot;]:checked {
  background: #3aaed8;
  color: #0b0d10;
}

/* fundo do dial */
QDial {
    background: black;
    border: 2px rgb(80,80,80);
}

/* a trilha (groove) */
QDial::groove {
    background: #2f333a;            /* cinza escuro */
    border: 2px solid #3a3f47;
    border-radius: 50%;             /* mantém circular */
    margin: 6px;                    /* espaço interno */
}

/* o preenchimento (chunk) do dial */
QDial::chunk {
    background: black;         /* azul claro */
    border-radius: 50%;
    margin: 6px;
}

/* o &quot;ponteiro&quot;/handle */
QDial::handle {
    background: black;            /* cor clara */
    border: 2px solid rgb(80,80,80);
    width: 18px;
    height: 18px;
    border-radius: 9px;
}
</string>
